{
	"name": "technical_test",
	"scripts": {
		"start": "ts-node src/index",
		"CLI": "ts-node src/CLI",
		"build": "tsc",
		"test": "mocha -r ts-node/register src/**/*.test.ts",
		"watch": "mocha -r ts-node/register --watch src/**/*.test.ts --watch-extensions ts",
		"coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test"
	},
	"devDependencies": {
		"@types/chai": "^4.2.3",
		"@types/mocha": "^5.2.7",
		"chai": "^4.2.0",
		"mocha": "^6.2.0",
		"nyc": "^14.1.1",
		"ts-node": "^8.4.1",
		"typescript": "^3.6.3"
	},
	"dependencies": {
		"@types/express": "^4.17.1",
		"@types/node": "^12.7.7",
		"@types/node-fetch": "^2.5.2",
		"body-parser": "^1.19.0",
		"express": "^4.17.1",
		"node-fetch": "^2.6.0"
	}
}
